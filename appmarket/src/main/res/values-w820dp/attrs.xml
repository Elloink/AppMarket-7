<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension"/>
        <attr name="border_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="TextProgressBar">

        <attr name="textColorPrimary" format="color"/>

        <attr name="textColorSecondary" format="color"/>

        <attr name="textSize" format="dimension"/>

        <attr name="backgroundColor" format="color"/>

        <attr name="primaryColor" format="color"/>

        <attr name="secondaryColor" format="color"/>

        <attr name="textProgress" format="boolean"/>

        <attr name="textNormal" format="boolean"/>

        <attr name="text" format="string"/>

    </declare-styleable>
</resources>

